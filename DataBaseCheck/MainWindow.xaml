<Window x:Class="DataBaseCheck.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataBaseCheck"
        mc:Ignorable="d"
        Title="MyDataGrid"  Height="600" Width="800" WindowStartupLocation="CenterScreen"
        MinHeight="600" MinWidth="800" Closing="Window_Closing"
        >
    <!--Data Source=DESKTOP-UV6LNDF;Initial Catalog=PhoneDB;Integrated Security=True-->

    <Grid AutomationProperties.Name="MyDataGrid">
        <Grid.Background>
            <ImageBrush ImageSource="Images/BackgroundAdd.jpg" Stretch="UniformToFill"/>
        </Grid.Background>
        <Frame  x:Name="FrameMain" Grid.Row="1" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
